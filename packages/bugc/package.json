{
  "name": "@ethdebug/bugc",
  "version": "0.1.0",
  "description": "The BUG language compiler with ethdebug/format debug information support",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "bugc": "./bin/bugc.ts"
  },
  "imports": {
    "#ast": "./dist/src/ast/index.js",
    "#ast/*": "./dist/src/ast/*.js",
    "#cli": "./dist/src/cli/index.js",
    "#cli/*": "./dist/src/cli/*.js",
    "#compiler": "./dist/src/compiler/index.js",
    "#compiler/*": "./dist/src/compiler/*.js",
    "#errors": "./dist/src/errors/index.js",
    "#errors/*": "./dist/src/errors/*.js",
    "#evm": "./dist/src/evm/index.js",
    "#evm/*": "./dist/src/evm/*.js",
    "#evmgen": "./dist/src/evmgen/index.js",
    "#evmgen/*": "./dist/src/evmgen/*.js",
    "#ir": "./dist/src/ir/index.js",
    "#ir/*": "./dist/src/ir/*.js",
    "#irgen": "./dist/src/irgen/index.js",
    "#irgen/*": "./dist/src/irgen/*.js",
    "#optimizer": "./dist/src/optimizer/index.js",
    "#optimizer/*": "./dist/src/optimizer/*.js",
    "#parser": "./dist/src/parser/index.js",
    "#parser/*": "./dist/src/parser/*.js",
    "#result": "./dist/src/result.js",
    "#typechecker": "./dist/src/typechecker/index.js",
    "#typechecker/*": "./dist/src/typechecker/*.js",
    "#types": "./dist/src/types/index.js",
    "#types/*": "./dist/src/types/*.js",
    "#test/*": "./dist/test/*.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src bin --ext .ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "yarn build && husky"
  },
  "keywords": [
    "ethereum",
    "compiler",
    "debugging",
    "ethdebug"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ethereumjs/common": "^10.0.0",
    "@ethereumjs/evm": "^10.0.0",
    "@ethereumjs/statemanager": "^10.0.0",
    "@ethereumjs/util": "^10.0.0",
    "@ethereumjs/vm": "^10.0.0",
    "@hyperjump/browser": "^1.2.0",
    "@hyperjump/json-schema": "^1.11.0",
    "@types/node": "^20.0.0",
    "@types/parsimmon": "^1.10.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.0.0",
    "ethereum-cryptography": "^3.2.0",
    "fast-check": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.0.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "fp-ts": "^2.16.11",
    "parsimmon": "^1.18.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
