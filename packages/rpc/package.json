{
  "name": "@ethdebug/rpc",
  "version": "0.1.0",
  "description": "RPC client utilities for Ethereum JSON-RPC",
  "keywords": [
    "ethereum",
    "rpc",
    "json-rpc",
    "web3"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json}'"
  },
  "dependencies": {
    "@ethdebug/bugc": "*",
    "ethereum-cryptography": "^2.1.2"
  },
  "bin": {
    "bug-deploy": "./bin/deploy.ts",
    "bug-call": "./bin/call.ts",
    "bug-storage": "./bin/storage.ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@ethdebug/format": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
